{
  "name": "voce-ui",
  "version": "1.0.0-alpha.0",
  "description": "A Vue.js 3.0 UI Toolkit for Web",
  "main": "index.js",
  "keywords": [
    "vue",
    "vue3",
    "Components",
    "UI"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "icons": "node scripts/iconfont-to-scss.js",
    "snippet": "node scripts/scss-to-snippet.js",
    "format": "prettier --write  \"{src,site}/**/*.{vue,js,ts,md,scss}\""
  },
  "homepage": "https://github.com/kitorv/voce-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitorv/voce-ui.git"
  },
  "bugs": {
    "url": "https://github.com/kitorv/voce-ui/issues"
  },
  "author": "kitorv",
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.6.0",
    "vue": "3.0.0"
  },
  "devDependencies": {
    "@vue/babel-plugin-jsx": "^1.0.0-rc.3",
    "@vue/cli-plugin-babel": "^4.5.9",
    "@vue/cli-plugin-typescript": "^4.5.9",
    "@vue/cli-service": "^4.5.9",
    "@vue/compiler-sfc": "3.0.0",
    "chalk": "^4.1.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "markdown-it": "^12.0.3",
    "markdown-it-container": "^3.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "request": "^2.88.2",
    "sass": "^1.30.0",
    "sass-loader": "^10.1.0",
    "typescript": "^4.1.3",
    "vue-loader": "^16.1.1",
    "vue-router": "^4.0.1"
  },
  "babel": {
    "presets": [
      "@vue/cli-plugin-babel/preset"
    ],
    "plugins": [
      "@vue/babel-plugin-jsx"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
