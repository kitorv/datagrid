{
  "name": "kv-table",
  "version": "0.1.0-alpha.1",
  "description": "A Table Component for Vue.js.",
  "keywords": [
    "vue",
    "table",
    "kv-table"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:style": "gulp build --gulpfile bulid/gulpfile.js",
    "build:script": "vue-cli-service  build --model production --target lib --dest library src/index.js",
    "build": "npm run build:script && npm run build:style",
    "docs": "vue-cli-service build --docs --dest docs",
    "lint": "vue-cli-service lint"
  },
  "main": "library/kv-table.common.js",
  "style": "style/index.css",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kitorv/kv-table.git"
  },
  "homepage": "https://kitorv.github.io/kv-table/",
  "bugs": {
    "url": "https://github.com/kitorv/kv-table/issues"
  },
  "devDependencies": {
    "vue": "^2.6.9",
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.3",
    "eslint-plugin-vue": "^5.2.2",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^4.0.2",
    "highlight.js": "^9.15.6",
    "markdown-it": "^8.4.2",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^2.0.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
